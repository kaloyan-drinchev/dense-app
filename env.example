# RevenueCat Configuration
# Copy this to .env when ready to configure RevenueCat
# Get these keys from RevenueCat Dashboard after setting up your Apple Developer account

# iOS API Key (starts with "appl_")
EXPO_PUBLIC_REVENUECAT_IOS_API_KEY=YOUR_IOS_API_KEY_HERE

# Android API Key (starts with "goog_") - for future use
EXPO_PUBLIC_REVENUECAT_ANDROID_API_KEY=YOUR_ANDROID_API_KEY_HERE

# Development settings
EXPO_PUBLIC_DEV_MODE=true
EXPO_PUBLIC_FORCE_LEGACY_SUBSCRIPTIONS=false

